{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning"
    },
    "IncludeLineBreak": false,
    "IncludeTimestamp": true,
    "IncludeZeroEventId": false,
    "IncludeLogNamespace": false,
    "TimestampPolicy": {
      "Format": "MM/dd/yyyy HH:mm:ss.fff"
    }
  },
  "HuaweiSun2000": {
    "Hostname": "solar-inverter.lan",
    "Inverter": {
      "ParametersToRead": [
        "Rated power [kW]",
        "Max power [kW]",
        "Max apparent power [kVA]",
        "Max reactive power to grid [kVar]",
        "Max apparent power from grid [kVar]",
        "Input power [kW]",
        "Voltage AB [V]",
        "Voltage BC [V]",
        "Voltage CA [V]",
        "Voltage A [V]",
        "Voltage B [V]",
        "Voltage C [V]",
        "Current A [V]",
        "Current B [V]",
        "Current C [V]",
        "Peak power today [kW]",
        "Power [kW]",
        "Reactive power [kVar]",
        "Power factor",
        "Grid frequency [Hz]",
        "Efficiency [%]",
        "Internal temperature [°C]",
        "Insulation resistance [MΩ]"
      ]
    },
    "InverterState": {
      "ParametersToRead": [
        "State 1: Standby",
        "State 1: Grid connected",
        "State 1: Grid connected normally",
        "State 1: Grid connection derated due to power rationing",
        "State 1: Grid connection derated due to internal cause",
        "State 1: Stop normal",
        "State 1: Stop due to faults",
        "State 1: Stop due to power rationing",
        "State 1: Shutdown",
        "State 1: Sport check",
        "State 2: Locked",
        "State 2: PV connected",
        "State 2: DSP data",
        "State 3: Off grid",
        "State 3: Off grid switch",
        "Device state: Standby: initializing",
        "Device state: Standby: detecting insulation resistance",
        "Device state: Standby: detecting irradiation",
        "Device state: Standby: grid detecting",
        "Device state: Starting",
        "Device state: On-grid (Off-grid mode: running)",
        "Device state: Grid connection: power limited (Off-grid mode: running: power limited)",
        "Device state: Grid connection: self-derating (Off-grid mode: running: self-derating)",
        "Device state: Shutdown: fault",
        "Device state: Shutdown: command",
        "Device state: Shutdown: OVGR",
        "Device state: Shutdown: communication disconnected",
        "Device state: Shutdown: power limited",
        "Device state: Shutdown: manual startup required",
        "Device state: Shutdown: DC switches disconnected",
        "Device state: Shutdown: rapid cutoff",
        "Device state: Shutdown: input underpower",
        "Device state: Grid scheduling: cos F-P curve",
        "Device state: Grid scheduling: Q-U curve",
        "Device state: Grid scheduling: PF-U curve",
        "Device state: Grid scheduling: dry contact",
        "Device state: Grid scheduling: Q-P curve",
        "Device state: Spot-check ready",
        "Device state: Spot-checking",
        "Device state: Inspecting",
        "Device state: AFCI self check",
        "Device state: I-V scanning",
        "Device state: DC input detection",
        "Device state: Running: off-grid charging",
        "Device state: Standby: no irradiation"
      ]
    },
    "PowerMeter": {
      "ParametersToRead": [
        "Power [W]",
        "Reactive power [VAR]",
        "Power factor",
        "Frequency [Hz]",
        "Exported energy [kWh]",
        "Accumulated energy [kWh]",
        "Power A [W]",
        "Power B [W]",
        "Power C [W]",
        "Voltage A [V]",
        "Voltage B [V]",
        "Voltage C [V]",
        "Current A [A]",
        "Current B [A]",
        "Current C [A]",
        "Voltage AB [V]",
        "Voltage BC [V]",
        "Voltage CA [V]"
      ]
    },
    "PVStrings": {
      "ParametersToRead": [
        "Voltage of all PV strings [V]",
        "Current of all PV strings [A]"
      ]
    }
  },
  "InfluxDB": {
    "Url": "http://influxserver:8086",
    "Token": "token",
    "Organization": "org",
    "Bucket": "solar"
  },
  "Worker": {
    "IntervalInSeconds": 5
  }
}