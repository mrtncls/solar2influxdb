{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning"
    },
    "IncludeLineBreak": false,
    "IncludeTimestamp": true,
    "IncludeZeroEventId": false,
    "IncludeLogNamespace": false,
    "TimestampPolicy": {
      "Format": "MM/dd/yyyy HH:mm:ss.fff"
    }
  },
  "HuaweiSun2000": {
    "Hostname": "solar-inverter.lan",
    "PowerMeter": {
      "ParametersToRead": [
        "Voltage A [V]",
        "Voltage B [V]",
        "Voltage C [V]",
        "Current A [A]",
        "Current B [A]",
        "Current C [A]",
        "Power [W]",
        "Reactive power [VAR]",
        "Power factor",
        "Frequency [Hz]",
        "Exported energy [kWh]",
        "Accumulated energy [kWh]",
        "Voltage AB [V]",
        "Voltage BC [V]",
        "Voltage CA [V]",
        "Power A [W]",
        "Power B [W]",
        "Power C [W]"
      ]
    }
  },
  "InfluxDB": {
    "Url": "http://influxserver:8086",
    "Token": "token",
    "Organization": "org",
    "Bucket": "solar"
  },
  "Worker": {
    "IntervalInSeconds": 5
  }
}